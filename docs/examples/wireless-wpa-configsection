Interface=wlan0
Connection=wireless
Security=wpa-configsection
# Uncomment this if your ssid is hidden
#Hidden=yes
IP='dhcp'
WPAConfigSection=(
    'ssid="University"'
    'key_mgmt=WPA-EAP'
    'eap=TTLS'
    'group=TKIP'
    'pairwise=TKIP CCMP'
    'anonymous_identity="anonymous"'
    'identity="myusername"'
    'password="mypassword"'
    'priority=1'
    'phase2="auth=PAP"'
)
